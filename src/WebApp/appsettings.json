{
  "Logging": {
    "LogLevel": {
      "Default": "Warning",
      "Microsoft": "Warning",
      "Microsoft.AspNetCore": "Warning",
      "System": "Warning"
    }
  },
  "AllowedHosts": "*",
  "EventBus": {
    "SubscriptionClientName": "Ordering.webapp"
  },
  "SessionCookieLifetimeMinutes": 60,
  "Telemetry": {
    "ServiceName": "webapp",
    "PerfMode": false
  },
  "OpenTelemetry": {
    "TraceSampleRatio": 0.05,
    "Otlp": {
      "Endpoint": "http://tempo:4317",
      "Protocol": "grpc"
    }
  },
  "Pyroscope": {
    "Server": "http://pyroscope:4040",
    "ApplicationName": "eshop.webapp",
    "SampleRate": 100,
    "UploadIntervalSeconds": 15
  },
  "OTEL_EXPORTER_OTLP_ENDPOINT": "http://tempo:4317",
  "OTEL_EXPORTER_OTLP_PROTOCOL": "grpc"
}
